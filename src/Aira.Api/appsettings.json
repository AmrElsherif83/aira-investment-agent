{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aira": "Information",
      "Aira.Core.Services.AnalysisService": "Information",
      "Aira.Api.Workers.AnalysisWorker": "Information"
    }
  },
  "AllowedHosts": "*",
  "Aira": {
    "DefaultTicker": "NVDA",
    "EnableVerboseStepArtifacts": true,
    "QueueCapacity": 100,
    "WorkerPollingIntervalSeconds": 2,
    "MaxConcurrentJobs": 1
  }
}
